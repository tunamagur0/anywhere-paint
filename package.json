{
  "name": "anywhere-paint",
  "version": "0.0.4",
  "description": "anywhere-paint is a library to create painting apps.",
  "keywords": [
    "paint",
    "drawing"
  ],
  "homepage": "https://github.com/tunamagur0/anywhere_paint/#readme",
  "bugs": {
    "email": "tsunamaguro3@gmail.com",
    "url": "https://github.com/tunamagur0/anywhere_paint/issues"
  },
  "main": "lib/anywherePaint.js",
  "types": "lib/anywherePaint.d.ts",
  "entry": "src/anywherePaint.ts",
  "files": [
    "lib",
    "README.md",
    "pacakge.json",
    "doc"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/tunamagur0/anywhere_paint.git"
  },
  "scripts": {
    "build": "npx webpack",
    "build:lib": "npx webpack --config webpack.config.lib.js",
    "build:clean": "rm -rf lib/ dist/",
    "start": "npx webpack-dev-server --open",
    "lint": "npx eslint \"src/**/*.ts\" --quiet --fix",
    "test": "npm run lint",
    "doc": "npx typedoc --out doc src",
    "prepare": "npm run test && npm run doc && npm run build:clean && npm run build:lib"
  },
  "author": {
    "name": "tunamagur0",
    "email": "tsunamaguro3@gmail.com"
  },
  "license": "MIT",
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^2.27.0",
    "@typescript-eslint/parser": "^2.27.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.1.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-import-resolver-webpack": "^0.12.1",
    "eslint-loader": "^4.0.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-prettier": "^3.1.2",
    "prettier": "^2.0.4",
    "ts-loader": "^6.2.1",
    "typedoc": "^0.17.3",
    "typescript": "^3.8.3",
    "webpack": "^4.41.5",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.10.3",
    "worker-loader": "^2.0.0"
  }
}